{
  "expo": {
    "name": "Lote Go",
    "slug": "lotego",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#38A169"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lotego.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#38A169"
      },
      "package": "com.lotego.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "51.0.0"
  }
}